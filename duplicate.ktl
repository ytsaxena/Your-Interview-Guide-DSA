fun main(){
  
  val arr = arrayOf(1, 2, 3, 1, 3, 6, 6)
  
  val alreadyseen = mutableSetOf<Int>()
  
  val duplicate = mutableSetOf<Int>()
  
  
  for(i in arr)
  {
    
    if(i in alreadyseen)
    {
       duplicate.add(i)
      
    }else{
      alreadyseen.add(i)
    }
    
  }
  
  
  println(duplicate)
  
  
  
  
  
  
  
}